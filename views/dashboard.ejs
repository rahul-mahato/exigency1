                            </tr>
                            <tr>
                                <td>Vehicle Number::</td>
                                <td><%= vno %></td>
                            </tr>
                            <tr>
                                <td>Aadhar::</td>
                                <td><%= aadhaar %></td>
                            </tr>
                            <tr>
                                <td>City::</td>
                                <td><%= city %></td>
                            </tr>
                            <tr>
                                <td>Address Line1::</td>
