<section id="hero-area" class="py-3">
    <div class="primary-overlay text-white">
        <div class="container">
            <div id="hero-banner" class="row align-item-center" style="display: none;">
                <div class="col">
                    <div class="text-center align-self-center align-center">
                        <img src="/images/hero-area-banner-icon.png" alt="" class="hero-png pb-2 img-fluid">
                    </div>
                    <div class="text-center align-self-center align-center">
                        <h1>Welcome
                            <%= Fname %>
                                <%= Lname %>
                        </h1>
                        <p class="lead">Welcome to your profile!</p>
                        <div class="icon-down"><i class="fas fa-chevron-down fa-2x pt-2"></i></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

